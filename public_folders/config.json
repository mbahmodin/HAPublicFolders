{
  "init": false,
  "slug": "public_folders",
  "name": "Public Folders",
  "version": "1.1.17",
  "description": "Server files on http.",
  "arch":
  [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "services",
  "boot": "auto",
  "map":
  [
    "media:ro"
  ],
  "ports":
  {
    "8443/tcp": 8443
  },
  "ports_description":
  {
    "8443/tcp": "Port to serve files on."
  },
  "options":
  {
    "folders":
    [
      "media:/media"
    ],
    "directory_listing": false,
    "request_logging": false
  },
  "schema":
  {
    "folders":
    [
      "str"
    ],
    "directory_listing": "bool",
    "request_logging": "bool"
  }
}
